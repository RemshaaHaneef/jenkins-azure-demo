pipeline {
    agent any
    stages {
        stage('Pull Code') {
            steps {
                git 'https://github.com/RemshaaHaneef/jenkins-azure-demo.git'
            }
        }
       stage('Build') {
    steps {
        echo 'Building Java project...'
        sh 'mvn clean install'
    }
}

      
        stage('Deploy') {
    steps {
        echo 'Deploying application...'
        sh 'cp target/*.jar /deploy/path/'
    }
}

    }
}
